{
  "hosting": {
    "public": "dist",
    "headers":[
      {
        "source":"**",
        "headers": [ {
          "key":"Access-Control-Allow-Origin",
          "value":"*"
        }]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}